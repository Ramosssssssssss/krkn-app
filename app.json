{
  "expo": {
    "name": "KRKN_G",
    "slug": "KRKN_G",
    "owner": "fyttsa",
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./assets/images/ggplay.png",
    "scheme": "krkng",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "Esta app necesita Bluetooth para conectar con impresoras térmicas",
        "NSBluetoothPeripheralUsageDescription": "Esta app necesita Bluetooth para conectar con impresoras térmicas"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/ggplay.png",
        "backgroundImage": "./assets/images/ggplay.png",
        "monochromeImage": "./assets/images/ggplay.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.NFC"
      ],
      "package": "com.black_sheep.KRKN_G"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/ggplay.png",
          "color": "#3B82F6",
          "defaultChannel": "inventarios",
          "sounds": [
            "./assets/sounds/aviso.wav"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ggplay.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-video",
      [
        "react-native-ble-plx",
        {
          "isBackgroundEnabled": true,
          "modes": [
            "peripheral",
            "central"
          ]
        }
      ],
      "react-native-nfc-manager"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "61009199-80c7-47f6-9ce6-b74902ece1f8"
      }
    }
  }
}